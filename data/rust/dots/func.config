# Functions
google() {
    if [ -z "$1" ]; then
        echo "No argument supplied"
        return
    fi
    echo "Searching for $@"
    search_string="$@"
    open "https://www.google.com/search?q=$search_string"
}

uwucd() {
  if [ -z "$1" ]; then
    echo "Where should I go? \(OwO\)"
    return
  fi
  
  unset CACHED_PATH;
  CACHED_PATH="$(pwd)";
  cd $@;
}

back() {
  unset CACHED_PATH;
  CACHED_PATH="$(pwd)";
  cd $CACHED_PATH;
}

cache() {
    echo $CACHED_PATH;
  }
